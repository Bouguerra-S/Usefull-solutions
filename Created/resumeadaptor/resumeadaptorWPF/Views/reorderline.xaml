<Window x:Class="resumeadaptorWPF.Views.reorderline"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:resumeadaptorWPF.Views"
        mc:Ignorable="d"
        Title="reorderline" Height="100" Width="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ComboBox Name="subCombo" Grid.Column="0">
        </ComboBox>
        <ComboBox Name="lineCombo" Grid.Column="1"/>
        <ComboBox Grid.Column="2">
            <ComboBoxItem>Up</ComboBoxItem>
            <ComboBoxItem>Down</ComboBoxItem>
        </ComboBox>
        <Button Grid.Column="3" Click="Button_Click">Reorder</Button>
    </Grid>
</Window>
