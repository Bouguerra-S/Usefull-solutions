<Window x:Class="resumeadaptorWPF.Views.reorderSection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:resumeadaptorWPF.Views"
        mc:Ignorable="d"
        Title="reorderSection" Height="100" Width="200">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        
        <ComboBox Name="wayCombo">
            <ComboBoxItem>Up</ComboBoxItem>
            <ComboBoxItem>Down</ComboBoxItem>
        </ComboBox>
        <Button Grid.Column="1" Click="Button_Click">Reorder</Button>
    </Grid>
</Window>
